{
  "name": "cronwatcher/laravel",
  "description": "CronWatcher.io Laravel integration package for monitoring and syncing scheduled tasks.",
  "keywords": ["laravel", "cron", "scheduler", "monitoring", "cronwatcher"],
  "type": "library",
  "license": "MIT",
  "version": "3.0.0",
  "homepage": "https://cronwatcher.io",
  "authors": [
    {
      "name": "cronWatcher",
      "email": "support@cronwatcher.io",
      "homepage": "https://github.com/cronwatcher/laravel"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "CronWatcher\\Laravel\\": "src/",
      "CronWatcher\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CronWatcher\\Laravel\\CronWatcherServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/cronwatcher/laravel/issues",
    "source": "https://github.com/cronwatcher/laravel"
  }
}
